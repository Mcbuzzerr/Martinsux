<p id="notice"><%= notice %></p>

<a href="https://fontmeme.com/wii-font/"><img src="https://fontmeme.com/permalink/180411/f78c40c531249a5fed5d240f721c3ace.png" alt="twiitr" border="0" class="antihover"></a>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @twiits.each do |twiit| %>
      <tr>
        <td><%= twiit.Title %></td>
        <td><%= twiit.Body %></td>
        <td><%= link_to 'Show', twiit %></td>
        <td><%= link_to 'Edit', edit_twiit_path(twiit) %></td>
        <td><%= link_to 'Destroy', twiit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Twiit', new_twiit_path %>
